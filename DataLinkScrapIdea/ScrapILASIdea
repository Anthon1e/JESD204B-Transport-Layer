input sof, eof, som, eom;

for (i = 0; i < 4; i++) begin
	if (sof[i]) begin 
		ilas_out[i*8 +: 8] = R;			
		next_is_q = 1;
	end else if (next_is_q && (mf_count == 1)) begin
		ilas_out[i*8 +: 8] = Q;
		next_is_q = 0;	
	end else if (eof[i]) begin
		ilas_out[i*8 +: 8] = A;
		mf_count++;
	end else if ((j < 14) && (mf_count == 1)) begin
		ilas_out[i*8 +: 8] = config_data[j*8 +: 8];
		j++;
	end else begin
		ilas_out[i*8 +: 8] = 8'h00;
	end
end
